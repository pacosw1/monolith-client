 (e, getTags) => {\n        getTags({variables: { keyword: e.target.value }})\n    }\n\n\n    const submitProduct = () => {\n        let input = {...fields, tags, images}\n\n        input.price = parseFloat(input.price)\n        input.freeStock = parseInt(input.freeStock)\n\n            createProduct({variables: { input }}).then(res => {\n                console.log(res)\n            }).catch(e => {\n                console.log(e)\n            })\n       \n    }\n\n    const onChangeField = (e) => {\n\n        let {value, name} = e.target\n\n        console.log(name)\n\n        let copy = {...fields}\n        copy[name] = value\n\n        setFields(copy)\n\n        console.log(fields)\n    }\n\n    console.log(images)\n\n\n\n\n    //INFO\n\n\n\n    //IMAGES\n\n\n\n    const removeImage = (name) => {\n\n        let copy = [...images]\n\n        copy = copy.filter(img => img.name !== name)\n\n        setImages(copy)\n    }\n\n    const updateImageFiles = (files) => {\n\n        files = Array.from(files)\n\n\n        setImages(files)\n    }\n\n\n\n    //TAGS\n    const addTag = (tag) => {\n\n        let copy = [...tags]\n        copy.push(tag)\n        \n        setTags(copy)\n\n    }\n\n    const removeTag = (id) => {\n\n        let copy = [...tags]\n\n        copy = copy.filter(x => x.id !== id)\n        \n        setTags(copy)\n\n    }\n\n    const renderTags = tags.map(tag => {\n        return <ExistingTag removeTag={removeTag} id={tag.id} key={tag.id} title={tag.title}/>\n    })\n\n    return <div>\n        <h1>Product Info</h1>\n        <TextInput onChange={onChangeField} name=\"title\"/>\n        <TextInput onChange={onChangeField} name=\"description\"/>\n        <NumberInput onChange={onChangeField} name=\"price\"/>\n        <NumberInput onChange={onChangeField} name=\"freeStock\"/>\n        <h1>Images</h1>\n        <ImageSection images={images} updateImageFiles={updateImageFiles} removeImage={removeImage}/>\n        \n        <h1>Tags</h1>\n        {renderTags}\n        <TagSection tags={tags} addTag={addTag} onSearch={onTagSearch}/>\n\n        <button onClick={() => submitProduct()}>Crear</button>\n    </div>\n}\n\n\n\n\n\n\n\n//IMAGES\n\nconst ImageSection = ({images, updateImageFiles, removeImage}) => {\n\n\n    const renderPreview = images.map((img, key) => {\n        return <Image src={URL.createObjectURL(img)} removeImage={removeImage} name={img.name} key={key} alt=\"preview\"/>\n    })\n\n    return <>\n\n        <input type=\"file\" multiple={true} onChange={ e => updateImageFiles(e.target.files) } />\n        {renderPreview}\n    </>\n}\n\nconst Image = ({src, name, alt, removeImage}) => {\n\n    return <img onClick={() => removeImage(name)} height={100} width={100} src={src} alt={alt}/>\n}\n\n\n\n\n//INPUTS\n\nconst NumberInput = ({onChange, name}) => {\n    return <input placeholder={name} name={name} type=\"number\" onChange={e => onChange(e)}/>\n}\nconst TextInput = ({onChange, name}) => {\n    return <input placeholder={name} name={name} type=\"text\" onChange={e => onChange(e)}/>\n}\n\n\n\n\n\n\n\n\n\n\n\n//TAGS\n\nconst TagSection = ({tags, addTag, onSearch}) => {\n\n\n    let [getTags, { loading, error, data }] = useLazyQuery(SEARCH_TAGS);\n\n    if (data) {\n\n        let hash = {}\n\n        tags.forEach(tag => {\n            if (!hash[tag.id]) {\n                hash[tag.id] = 1\n            }\n        });\n        let copy = [...data.searchTags]\n\n        data = copy.filter(x => !hash[x.id]) \n    }\n\n\n    const renderTags = (data ? data: []).map(tag => {\n        return <Tag addTag={addTag} key={tag.id} id={tag.id} title={tag.title}/>\n    })\n\n\n    return <div>\n        <h1>{error ? \"Error connecting to server\" : null} </h1>\n        <h1>Select Tags</h1>\n        <SearchBar onChange={e => onSearch(e, getTags)}/>\n\n        {renderTags}\n        \n    </div>\n}\n\n\n\n//tags\n\nconst ExistingTag = ({id, title, removeTag}) => {\n\n    return <h1 onClick={() => removeTag(id)}>{title}</h1>\n}\n\n\nconst Tag = ({id, title, addTag}) => {\n\n    return <h1 onClick={() => addTag({id, title})}>{id} {title}</h1>\n}\n\nexport default NewProduct","/Users/paquitaladelbarrio/Documents/pr-hummus/monolith-client/src/views/login/styles.js",[],"/Users/paquitaladelbarrio/Documents/pr-hummus/monolith-client/src/components/button/button.js",[],"/Users/paquitaladelbarrio/Documents/pr-hummus/monolith-client/src/views/login/queries.js",[],"/Users/paquitaladelbarrio/Documents/pr-hummus/monolith-client/src/views/weather/index.js",[],"/Users/paquitaladelbarrio/Documents/pr-hummus/monolith-client/src/views/weather/styles.js",[],"/Users/paquitaladelbarrio/Documents/pr-hummus/monolith-client/src/components/spinner/index.js",[],"/Users/paquitaladelbarrio/Documents/pr-hummus/monolith-client/src/views/home/index.js",["79","80"],{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","replacedBy":"84"},{"ruleId":"85","severity":1,"message":"86","line":2,"column":10,"nodeType":"87","messageId":"88","endLine":2,"endColumn":19},{"ruleId":"85","severity":1,"message":"89","line":53,"column":19,"nodeType":"87","messageId":"88","endLine":53,"endColumn":29},{"ruleId":"85","severity":1,"message":"90","line":1,"column":10,"nodeType":"87","messageId":"88","endLine":1,"endColumn":22},{"ruleId":"85","severity":1,"message":"86","line":2,"column":27,"nodeType":"87","messageId":"88","endLine":2,"endColumn":36},{"ruleId":"85","severity":1,"message":"91","line":15,"column":40,"nodeType":"87","messageId":"88","endLine":15,"endColumn":51},{"ruleId":"92","severity":1,"message":"93","line":43,"column":22,"nodeType":"94","messageId":"95","endLine":43,"endColumn":24},{"ruleId":"92","severity":1,"message":"93","line":43,"column":40,"nodeType":"94","messageId":"95","endLine":43,"endColumn":42},{"ruleId":"85","severity":1,"message":"86","line":4,"column":17,"nodeType":"87","messageId":"88","endLine":4,"endColumn":26},{"ruleId":"85","severity":1,"message":"96","line":21,"column":38,"nodeType":"87","messageId":"88","endL